<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="javarajob.repository.FavCareerDao">
	<resultMap type="favCareer" id="favCareerList"/>	
	<insert id="addFavCareer" parameterType="favCareer">
		insert into FAVCAREER values(#{careerid},#{id})
	</insert>
	<delete id="removeFavCareer" parameterType="int" timeout="20">
		delete favcareer where careerid = #{careerid}
	</delete>
</mapper>